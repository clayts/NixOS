  "go.toolsManagement.autoUpdate": true,
  "evenBetterToml.semanticTokens": true,
  "editor.stickyScroll.maxLineCount": 10,
  "gtkTitleBar.mode": "dark",
  "editor.stickyScroll.enabled": true,
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "editor.inlayHints.enabled": "offUnlessPressed",
  "go.inlayHints.rangeVariableTypes": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.assignVariableTypes": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "rust-analyzer.cargo.buildScripts.rebuildOnSave": true,
  "rust-analyzer.completion.autoself.enable": false,
  "rust-analyzer.completion.autoimport.enable": false,
  "rust-analyzer.completion.fullFunctionSignatures.enable": true,
  "rust-analyzer.diagnostics.previewRustcOutput": true,
  "rust-analyzer.hover.actions.debug.enable": false,
  "rust-analyzer.hover.actions.references.enable": true,
  "rust-analyzer.imports.granularity.enforce": true,
  "rust-analyzer.imports.prefix": "crate",
  "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.inlayHints.closingBraceHints.minLines": 1,
  "rust-analyzer.inlayHints.closureCaptureHints.enable": true,
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
  "rust-analyzer.inlayHints.discriminantHints.enable": "always",
  "rust-analyzer.inlayHints.implicitDrops.enable": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  "rust-analyzer.inlayHints.rangeExclusiveHints.enable": true,
  "rust-analyzer.lens.references.trait.enable": true,
  "rust-analyzer.lens.references.method.enable": true,
  "rust-analyzer.lens.references.enumVariant.enable": true,
  "rust-analyzer.lens.references.adt.enable": true,
  "rust-analyzer.restartServerOnConfigChange": true,
  "rust-analyzer.semanticHighlighting.operator.specialization.enable": true,
  "rust-analyzer.semanticHighlighting.punctuation.specialization.enable": true,
  "rust-analyzer.semanticHighlighting.punctuation.separate.macro.bang": true,
  "rust-analyzer.semanticHighlighting.punctuation.enable": true,
  // "rust-analyzer.trace.extension": true,
  "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
  "rust-analyzer.workspace.symbol.search.kind": "all_symbols",
  // "rust-analyzer.server.path": "rust-analyzer",
  // "rust-analyzer.server.path": "/usr/lib/rustup/bin/rust-analyzer",
  "rust-analyzer.check.command": "clippy",
  "errorLens.removeLinebreaks": false,
  "errorLens.enabledDiagnosticLevels": [
    "error",
    "warning",
    "info",
    "hint"
  ],
  "errorLens.delayMode": "debounce",
  "errorLens.alignMessage": {
    "start": 0,
    "end": 0
  },
  "errorLens.messageTemplate": "<<- $message",
  "errorLens.decorations": {
    "errorRange": {
      "backgroundColor": "#5F1010ff"
    },
    "warningRange": {
      "backgroundColor": "#883500ff"
    },
    "infoRange": {
      // "backgroundColor": "#ffffff"
    },
    "hintRange": {
      "backgroundColor": "#003f3fff"
    }
  },
  "errorLens.problemRangeDecorationEnabled": true,
  "window.menuBarVisibility": "toggle",
  "editor.codeLens": false,
  // "editor.fontWeight": "350",
  "editor.renderLineHighlight": "gutter",
  // "rust-analyzer.check.features": "all",
  "rust-analyzer.cargo.features": "all",
  "markdown.preview.typographer": true,
  "markdown-pdf.displayHeaderFooter": false,
  "markdown-pdf.includeDefaultStyles": false,
  "html.format.preserveNewLines": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "nix.enableLanguageServer": true,
  "nix.serverSettings": {
    "nil": {
      "formatting": {
        "command": [
          "alejandra"
        ]
      }
    }
  },
  "symbols.hidesExplorerArrows": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.bracketPairsHorizontal": false,
  "direnv.restart.automatic": true,
  "git.autofetch": true,
  "window.titleBarStyle": "custom",
  "terminal.external.linuxExec": "kgx",
  "diffEditor.ignoreTrimWhitespace": true,
  "terminal.integrated.customGlyphs": false,
  "workbench.iconTheme": "file-icons",
  "workbench.colorTheme": "Adwaita Dark",
  "terminal.integrated.smoothScrolling": true,
  "git.terminalGitEditor": true
